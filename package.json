{
	"name": "vanadium",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"type-check": "tsc",
		"dev": "vite dev",
		"dev_inspect": "NODE_OPTIONS=\"--inspect\" vide dev",
		"build": "npx prisma generate && vite build",
		"preview": "vite preview --port 80 --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^20.11.20",
		"@types/serviceworker": "^0.0.84",
		"autoprefixer": "^10.4.17",
		"postcss": "^8.4.33",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"prisma": "^5.9.1",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vite-plugin-inspect": "^0.8.3"
	},
	"type": "module",
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.0",
		"@nolanx/svelte-onesignal": "^2.0.5",
		"@onesignal/node-onesignal": "2.0.1-beta2",
		"@prisma/client": "^5.9.1",
		"@types/mysql": "^2.15.25",
		"chart.js": "^3.9.1",
		"exceljs": "^4.4.0",
		"mysql2": "^3.9.1",
		"npm": "^10.5.1",
		"npx": "^10.2.2",
		"oslo": "^1.1.3",
		"reflect-metadata": "^0.2.2",
		"svelte-kit-cookie-session": "^4.0.0",
		"tailwindcss-gradients": "^3.0.0",
		"vavite": "^4.1.1",
		"zod": "^3.22.4"
	}
}
